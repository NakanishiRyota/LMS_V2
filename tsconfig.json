{
  "compilerOptions": {
      "typeRoots": [
      "types",
      "node_modules/@types"
    ],
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "strict": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"
  },
  "include": ["src"]
}
